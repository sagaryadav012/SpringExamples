<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
     
     <bean id="address1" class="com.learning.DIBySetterEx2.Address">
       <property name="city" value="Hyderabad"></property>
       <property name="state" value="Telangana"></property>
       <property name="country" value="India"></property>
     </bean>
         
     <bean id="emp" class="com.learning.DIBySetterEx2.Employee">
       <property name="id" value="901"></property>
       <property name="name" value="Sharath Goud"></property>
       <property name="address" ref="address1"></property>
     </bean>

</beans>