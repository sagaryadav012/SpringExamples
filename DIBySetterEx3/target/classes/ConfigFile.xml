<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
         
     <bean id="institute" class="com.learning.DIBySetterEx3.Institute">
       <property name="name" value="Naresh IT"></property>
       <property name="area" value="Ameerpet"></property>
       <property name="courses">
       <list>
        <value>Full Stack JAVA Developer</value>
        <value>Azure Cloud</value>
        <value>Web Developer</value>
        <value>Data Science</value>
       </list>
       </property>
     </bean>

</beans>